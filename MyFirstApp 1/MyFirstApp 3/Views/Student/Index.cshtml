@model List<Student>

<h2 style="text-align: center;">List Of Students</h2>

<table border="2" border-spacing= "15px", style="width: 100%;"> 
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Age</th>
    </tr>
    @foreach (var student in Model)
    {
        <tr>
            <td>@student.Id</td>
            <td>@student.Name</td>
            <td>@student.Age</td>
        </tr>
    }
</table>
